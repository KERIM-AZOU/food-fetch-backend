# Backend Environment Variables
# Copy this file to .env and fill in your API keys

# Server Configuration
FETCH_PORT=3000

# Groq API - Used for transcription (Whisper) and AI extraction
# Get your key at https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# ElevenLabs API - Used for text-to-speech
# Get your free key at https://elevenlabs.io
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google Gemini API - Native audio processing (alternative to Groq/ElevenLabs)
# Get your key at https://aistudio.google.com
GEMINI_API_KEY=your_gemini_api_key_here

# Toggle between Groq/ElevenLabs and Gemini
# Set to 'true' to use Gemini 2.5 Flash Native Audio, 'false' for Groq/ElevenLabs
USE_GEMINI=false
